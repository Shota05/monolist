<% if params[:type] == "Have" %>
  $(".have-<%= @item.id %>,.have-<%= @item.asin %>").replaceWith("<%= escape_javascript(render('items/unhave' , item: @item)) %>");
  $("#have_count").html('<%= current_user.have_items.count %>');
<% else %>
  $(".want-<%= @item.id %>,.want-<%= @item.asin %>").replaceWith("<%= escape_javascript(render('items/unwant' , item: @item)) %>");
  $("#want_count").html('<%= current_user.want_items.count %>');
<% end %>

$('.btn-have,.btn-want').removeAttr('disabled');